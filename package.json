		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"chai": "^5.1.1",
		"esbuild": "0.17.3",
		"mocha": "^10.7.3",
		"obsidian": "latest",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "^4.7.4"
